{
	"name": "@antithrow/eslint-plugin",
	"version": "1.2.1",
	"description": "ESLint plugin for antithrow Result types",
	"license": "MIT",
	"author": "Jack Weilage",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/antithrow/antithrow.git",
		"directory": "packages/eslint-plugin"
	},
	"homepage": "https://antithrow.dev",
	"bugs": {
		"url": "https://github.com/antithrow/antithrow/issues"
	},
	"keywords": [
		"antithrow",
		"eslint",
		"eslintplugin",
		"eslint-plugin",
		"result",
		"error-handling",
		"typescript"
	],
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"clean": "rm -rf dist",
		"lint": "bun run lint:publint && bun run lint:types",
		"lint:publint": "publint",
		"lint:types": "tsc --noEmit"
	},
	"dependencies": {
		"@typescript-eslint/utils": "8.55.0"
	},
	"devDependencies": {
		"@types/node": "25.2.3",
		"@typescript-eslint/parser": "8.55.0",
		"@typescript-eslint/rule-tester": "8.55.0",
		"antithrow": "workspace:^",
		"eslint": "9.39.2",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"eslint": "^9.0.0",
		"typescript": ">=5.0.0"
	}
}
